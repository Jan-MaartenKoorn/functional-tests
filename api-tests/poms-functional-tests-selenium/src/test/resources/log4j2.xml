<?xml version="1.0" ?>
<Configuration status="INFO">
   <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{ISO8601}&#009;%4.4p&#009;%X{testNumber}%X{currentTest}&#009;%m%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>
